<div class="card mb-2">
  <div class="card-body">
    <%= link_to 'X', [@org, @season, @league, team],
            class: 'float-right btn btn-sm btn-danger',
            method: :delete,
            data: { confirm: 'Are you sure you want to destroy ' + team.name + '?'} %>
    <h3><%= link_to (team.name), [@org, @season, @league, team] %></h3>
    <%= link_to 'Edit', [:edit, @org, @season, @league, team], class: 'float-right btn btn-sm btn-secondary' %>

  </div>
</div>
